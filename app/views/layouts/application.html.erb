<!DOCTYPE html>
<html>
<head>
  <title>Photoman</title>
  <%= stylesheet_link_tag    "application", :media => "all" %>
  <%= javascript_include_tag "application" %>
  <%= csrf_meta_tags %>
</head>
<body>
<%= render :partial => 'menu' %>
<%= yield %>

<h2>Autoalbums (<%= @images.count %> photos) </h2>


<% @images.each do |i| %>
  <p><img src="<%= i[:url] %>" /><%= i[:exif] %></p>
<% end %>

</body>
</html>
